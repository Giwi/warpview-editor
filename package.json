{
  "name": "@senx/warpview-editor",
  "version": "1.0.12",
  "description": "WarpScript editor",
  "repository": {
    "type": "git",
    "url": "https://github.com/senx/warpview-editor.git"
  },
  "author": "SenX <contact@senx.io>",
  "license": "Apache-2.0",
  "keywords": [
    "web",
    "editor",
    "code",
    "web components",
    "time series",
    "warp10",
    "Warp 10",
    "WarpScript",
    "Geo Times Series",
    "Times Series"
  ],
  "module": "dist/esm/index.js",
  "main": "dist/index.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "sd concurrent \"node --max_old_space_size=4096 --optimize_for_size --stack_size=4096 ./node_modules/@stencil/core/bin/stencil build --prod\"",
    "dev": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll"
  },
  "dependencies": {
    "@giwisoft/wc-tabs": "^0.0.1",
    "@timkendrick/monaco-editor": "0.0.9",
    "monaco-editor": "0.15.6",
    "whatwg-fetch": "3.0.0"
  },
  "devDependencies": {
    "@stencil/core": "~0.16.1",
    "@stencil/sass": "^0.1.1",
    "@stencil/utils": "^0.0.5"
  },
  "bugs": {
    "url": "https://github.com/senx/warpview-editor/issues"
  },
  "homepage": "https://www.warp10.io"
