{
  "name": "@senx/warpview-editor",
  "version": "1.2.1",
  "description": "WarpScript editor",
  "repository": {
    "type": "git",
    "url": "https://github.com/senx/warpview-editor.git"
  },
  "author": "SenX <contact@senx.io>",
  "license": "Apache-2.0",
  "keywords": [
    "web",
    "editor",
    "code",
    "web components",
    "time series",
    "warp10",
    "Warp 10",
    "WarpScript",
    "Geo Times Series",
    "Times Series"
  ],
  "module": "dist/esm/index.js",
  "main": "dist/warpview-editor.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "ng": "ng",
    "dev": "yarn copy && ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "copy": "cp -r node_modules/monaco-editor/min/vs src/vs",
    "build:elements": "node --max_old_space_size=4096 --optimize_for_size --stack_size=4096 ./node_modules/@angular/cli/bin/ng build elements --prod && node scripts/build-elements.js",
    "gen": "cd src/app/elements/ && ng g component --module=elements.module.ts --entryComponent=true --skipTests=true --style=scss --viewEncapsulation=ShadowDom --export=true"
  },
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/elements": "^7.2.15",
    "@angular/forms": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "@giwisoft/wc-split": "0.0.7",
    "@giwisoft/wc-tabs": "0.0.10",
    "@webcomponents/custom-elements": "^1.2.4",
    "abortcontroller-polyfill": "^1.3.0",
    "core-js": "^2.5.4",
    "document-register-element": "^1.14.3",
    "monaco-editor": "^0.18.1",
    "resize-observer-polyfill": "^1.5.1",
    "rxjs": "^6.4.0",
    "rxjs-compat": "^6.4.0",
    "tslib": "^1.9.0",
    "whatwg-fetch": "3.0.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular-devkit/core": "^7.3.9",
    "@angular/cli": "~7.3.9",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@types/jest": "^21.1.1",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "copy-webpack-plugin": "^5.0.4",
    "fs-extra": "^8.0.1",
    "jest": "^21.2.1",
    "ngx-build-plus": "^8.1.4",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-stencil": "^1.0.0",
    "typescript": "~3.2.2"
  },
  "bugs": {
    "url": "https://github.com/senx/warpview-editor/issues"
  },
  "homepage": "https://www.warp10.io",
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
